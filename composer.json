{
    "name": "aoe/restler",
    "type": "typo3-cms-extension",
    "version": "1.2.12",
    "description": "A TYPO3-Extension, that integrates the popular PHP REST-framework Restler in TYPO3.",
    "homepage": "https://typo3.org/extensions/repository/view/restler",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "AOE GmbH",
            "email": "dev@aoe.com"
        }
    ],
    "require": {
        "php": ">=5.3.4",
        "typo3/cms-core": ">=6.2.14",
        "luracast/restler": "3.0.0.20160213"
    },
    "require-dev": {
        "typo3/cms": "6.2.*",
        "phpunit/phpunit": "4.6.*",
        "phpunit/phpcov": "2.0.*",
        "squizlabs/php_codesniffer": "2.3.*",
        "sebastian/phpcpd": "2.0.*",
        "phpmd/phpmd": "2.2.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AOEpeople/Restler.git"
        }
    ],
    "replace": {
        "restler": "*"
    },
    "keywords": [
        "TYPO3",
        "Restler",
        "REST",
        "API"
    ]
}
