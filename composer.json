{
    "name": "aoe/restler",
    "type": "typo3-cms-extension",
    "description": "A TYPO3-Extension, that integrates the popular PHP REST-framework Restler in TYPO3.",
    "homepage": "https://typo3.org/extensions/repository/view/restler",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "AOE GmbH",
            "email": "dev@aoe.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "typo3/cms-core": ">=6.2.14",
        "luracast/restler": "~3.0.0@RC"
    },
    "require-dev": {
        "typo3/cms": "6.2.*",
        "phpunit/phpunit": "4.6.*",
        "phpunit/phpcov": "2.0.*",
        "squizlabs/php_codesniffer": "2.3.*",
        "sebastian/phpcpd": "2.0.*",
        "phpmd/phpmd": "2.2.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AOEpeople/Restler.git"
        }
    ],
    "replace": {
        "restler": "*"
    },
    "keywords": [
        "TYPO3",
        "Restler",
        "REST",
        "API"
    ],
    "autoload": {
        "psr-4": {
            "Aoe\\Restler\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aoe\\Restler\\Tests\\": "Tests",
            "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/"
        }
    }
}
